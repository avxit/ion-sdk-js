{
  "name": "ion-sdk-js",
  "version": "1.8.0",
  "description": "A js sdk for ion-sfu or ion",
  "keywords": [
    "pion",
    "sfu",
    "webrtc",
    "ion",
    "js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/*",
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "bundle": "webpack",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.(ts|js)\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && npm run bundle",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "web-dev": "webpack serve --mode development --open --config ./webpack.dev.config.js --hot --inline --port 8081 --host localhost"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.2",
    "@improbable-eng/grpc-web": "^0.15.0",
    "browser-headers": "^0.4.1",
    "events": "^3.3.0",
    "google-protobuf": "^3.19.1",
    "protoc-gen-ts": "^0.6.0",
    "sdp-transform": "^2.14.1",
    "uuid": "^8.3.2",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/runtime": "7.16.0",
    "@types/events": "^3.0.0",
    "@types/google-protobuf": "3.15.5",
    "@types/node": "16.11.6",
    "@types/sdp-transform": "2.4.5",
    "@types/uuid": "8.3.1",
    "prettier": "2.4.1",
    "ts-loader": "8.3.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-dev-server-output": "0.0.6"
  }
}
